{
    "rewrites": [
      {
        "source": "/workspace/:workspaceId",
        "destination": "/index.html"
      },
      {
        "source": "/workspace/:workspaceId/tasks",
        "destination": "/index.html"
      },
      {
        "source": "/workspace/:workspaceId/members",
        "destination": "/index.html"
      },
      {
        "source": "/workspace/:workspaceId/reports",
        "destination": "/index.html"
      },
      {
        "source": "/workspace/:workspaceId/reports/:reportId",
        "destination": "/index.html"
      },
      {
        "source": "/workspace/:workspaceId/settings",
        "destination": "/index.html"
      },
      {
        "source": "/workspace/:workspaceId/project/:projectId",
        "destination": "/index.html"
      },
      {
        "source": "/invite/workspace/:inviteCode/join",
        "destination": "/index.html"
      },
      {
        "source": "/sign-up",
        "destination": "/index.html"
      },
      {
        "source": "/google/oauth/callback",
        "destination": "/index.html"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  